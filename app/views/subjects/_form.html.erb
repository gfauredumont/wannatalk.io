
<%=form_for(@subject) do |f| %>
  <div class="form-group">  <%= f.label :titre%>
    <%= f.text_field :title%>
  <div class="form-group">
    <%= f.label :description%>
    <%= f.text_field :description%>
  <div class="form-group">
    <%= f.collection_select :conference_id, Conference.all, :id, :title%>
    <%= f.hidden_field :questioner_id, :value => current_participant.id %>
  <div class="form-group">
    <%= f.submit "Enregistrer"%>
<% end %>
